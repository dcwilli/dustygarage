unsold = ""

    tool = Tool.query.filter(
        Tool.sold_status != unsold).filter_by(user_id=userid).all()
    print(tool)

    # query db for tools current user has sold
    bids = db.session.query(Tool, Bid).join(
        Bid).filter_by(user_id=userid).all()
    print(bids)
    current_user = session.get('user_id')


#in tools.py  create route
@bp.route('/userdash/<userid>', methods=["POST", "GET"])
@login_required
def userdash(userid):

    print(userid)
    unsold = ""

    tool = Tool.query.filter(
        Tool.sold_status != unsold).filter_by(user_id=userid).all()
    print(tool)

    
    # if the url userid does not match the logged in user - log them out
    # if current_user != userid:
    #     return redirect(url_for('auth.logout'))

    return render_template('tools/sellhistory.html', userid=userid, tool=tool, bids=bids)



    